#include <stdio.h>
main()
{
	int a,b,c,d=0,e=0;
	printf("Enter The Input(N):");
	scanf("%d",&a);
	for(b=1;b<1000000000000;b++)
	{
		d=0;
		if(e<a)
		{
			for(c=1;c<b;c++)
			{
				if(b%c==0)
				{
					d+=c;
			    }	
			}
			if(d==b)
			{
			printf("%d",b);
			e+=1;
			if(e<a)
			{
				printf(",");
			}
			}
	    }
	}
}
