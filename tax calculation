#include <stdio.h>
main()
{
	int a,b,c,d,e;
	printf("Enter The Income:");
	scanf("%d",&a);
	if(a<=150000 && a>=0)
	{
		printf("Tax=%d",0);
	}
	else if(a>=150001 && a<300000)
	{
		printf("Tax=%d",a/10);
	}
	else if(a>=300001 && a<500000)
	{
		printf("Tax=%d",a/5);
	}
	else if(a>500001)
	{
		printf("Tax=%d",(a*3)/100);
	}
	else
	{
		printf("Enter Valid Input");
	}
}
