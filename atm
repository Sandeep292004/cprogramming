#include <stdio.h>
main()
{
	int a,b,c,d,e,f,g,h;
	printf("Available Denominations 2000,500,200,100\n");
	printf("Enter The First Denomination:");
	scanf("%d",&a);
	printf("Enter the First Denominaion Notes:");
	scanf("%d",&e);
	printf("Enter The Second Denomination:");
	scanf("%d",&b);
	printf("Enter the Second Denominaion Notes:");
	scanf("%d",&f);
	printf("Enter The Third Denomination:");
	scanf("%d",&c);
	printf("Enter the Third Denominaion Notes:");
	scanf("%d",&g);
	printf("Enter The Fourth Denomination:");
	scanf("%d",&d);
	printf("Enter the Fourth Denominaion Notes:");
	scanf("%d",&h);
	printf("Total Available Balance In Atm:%d",(a*e)+(b*f)+(c*g)+(d*h));
}
